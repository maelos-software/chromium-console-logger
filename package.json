{
  "name": "chromium-console-logger",
  "version": "1.0.0",
  "description": "A standalone local developer tool that connects to Chromium-based browsers via CDP to capture console events and exceptions to NDJSON files",
  "main": "dist/index.js",
  "bin": {
    "chromium-console-logger": "./bin/chromium-console-logger"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/chromium-console-logger.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/chromium-console-logger/issues"
  },
  "homepage": "https://github.com/yourusername/chromium-console-logger#readme",
  "scripts": {
    "build": "tsc && npx esbuild src/tui-runner.mjs --bundle --platform=node --format=esm --outfile=dist/tui-runner.mjs --external:ink --external:react --loader:.mjs=jsx",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cdp",
    "chrome-devtools-protocol",
    "console",
    "logging",
    "vivaldi",
    "chrome",
    "browser",
    "debugging"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^19.2.7",
    "chrome-remote-interface": "^0.33.0",
    "commander": "^11.1.0",
    "ink": "^6.5.1",
    "react": "^19.2.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
